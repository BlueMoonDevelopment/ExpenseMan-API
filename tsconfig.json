{
  "compilerOptions": {                        
    "target": "es6",                               
    "module": "commonjs",                           
    "outDir": "./",                             
    "rootDirs": ["./"],                             
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,                       
  },
  "exclude":[
    "./node_modules",
    "./public",
    "./openapi-docs"
  ]
}
